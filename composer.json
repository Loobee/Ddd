{
  "name": "loobee/ddd",
  "description": "Domain-Driven Design library",
  "keywords": ["DDD", "domain driven design"],
  "homepage": "https://github.com/Loobee/Ddd",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Savitsky",
      "email": "qRoC.Work@gmail.com"
    }
  ],
  "type": "library",
  "autoload": {
    "psr-4": {"Loobee\\Ddd\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Loobee\\Ddd\\": "tests/"}
  },
  "require": {
    "php": ">=7.0",
    "jms/serializer"   : "~1.1",
    "beberlei/assert"  : "~2.4",
    "ramsey/uuid"      : "~3.1"
  },
  "suggest": {
    "symfony/symfony" : "Integration with Symfony",
    "doctrine/orm"    : "Integration with Doctrine",
    "php-amqplib/php-amqplib" : "For support notification system",
    "react/event-loop" : "For support notification system"
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
